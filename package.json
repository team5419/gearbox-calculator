{
  "name": "gearbox-calculator",
  "version": "1.0.0",
  "description": "An application for calculations related to single speed gear boxes",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "reinstall": "rm -rf ./node_modules/ && npm install && npm start",
    "compress": "electron-builder --dir",
    "package": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "frc",
    "gearbox"
  ],
  "author": "Team 5419",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.4.0"
  },
  "dependencies": {
    "jquery": "^3.4.1"
  },
  "build": {
    "asar": true,
    "productName": "Gearbox Calculator",
    "win": {
      "target": "portable",
      "icon": "./build/icon.ico",
      "publisherName": "5419"
    }
  }
}